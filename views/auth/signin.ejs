<div id="signin-main">
    <div id="signin-info">
    <div id="signin-logo"></div>

<h1> Sign In </h1>

<form method="POST" action="/auth/signin">

    <div class="form-group">
        <label class="signin-label">Email Address</label><br>
        <input type="email" name="emailAddress" class="signin-input" />
    </div>

    <div class="form-group">
        <label class="signin-label">Password</label><br>
        <input type="password" name="password" class="signin-input" />
    </div>

    <br>

    <div>
        <button type="submit" class="btn btn-primary">Sign In</button>
    </div>
    <br>

    <div>
        <a href="/auth/forget">Forget Password</a>
    </div>

</form>
</div>

</div>