<h1>select account</h1>

<% account.forEach((e,k)=>{ %>
    <h1>
        <%= k + 1 %>
    </h1>
    <%= e.accountName %>
        <%= e.accountNumber %>
            <%= e.currency %>
                <%= e.balance %>
                    <%= e.transactions %>
                        <%= e.user %>
                            <a href="/transaction/add?senderAccount=<%= e.accountNumber %>">send money</a>
                            <% }) %>