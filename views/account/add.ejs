<form method="post" action="/account/add">
    <div>
        <label>Account Name</label>
        <input name="accountName"  type="text"/>
    </div>
    
    <div class="form-group">
        <label>Account Number</label>
        <input name="accountNumber" type="text" id="anumber" readonly/>
        </div>
    
    
        <div class="form-group">
        <label>Currency</label>
        <!-- BHD, USD, GBP -->
            <select name="currency">
                <option>BHD</option>
                <option>USD</option>
                <option>GBP</option>
            </select>
        </div>

        <div class="form-group">
        <label>Balance</label>
        <input name="balance" type="number"/>
        </div>

        


    
       
    <br>
    <div>
    <button type="submit">Create Account</button>
    </div>

</form>
